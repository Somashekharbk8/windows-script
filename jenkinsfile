pipeline {
	agent any
		stages {
			stage ('Build') {
				steps {
						sh 'echo this is my pipeline script'
						sh 'ls '
						sh 'free'
						sh 'ls -lrt'
						sh 'df -h'
						}
					}
			stage ('TEST') {
					steps {
							sh 'echo this is for test'
							sh 'df -h'
							sh 'git --version'
							sh 'ls -a'
							}
						}
			stage ('DEPLOY') {
					steps {
							sh 'du -h'
							sh 'ls -lrt'
							sh 'echo first webhook'
							sh 'echo this is for build'
							}

						}
				}
		}
